{
  "name": "gitlabot",
  "version": "0.0.11",
  "description": "A gitlab update bot",
  "main": "index.js",
  "dependencies": {
    "ascii-table": "0.0.9",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.1",
    "telebot": "^1.4.1",
    "winston": "^3.3.3",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "nodemon": "^2.0.5"
  },
  "scripts": {
    "prestart": "npx prettier --write '*.js'",
    "start": "npm i && node index.js",
    "start.dev": "npm ci && nodemon index.js --debug=true",
    "start.devmon": "nodemon index.js --debug=true --exec \"npx prettier --write '*.js' && node\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SaphireVert/gitlabot.git"
  },
  "keywords": [
    "gitlab",
    "bot",
    "telegram",
    "telebot",
    "news",
    "update",
    "EPFL",
    "saphirevert",
    "pyrosaphire"
  ],
  "author": "SaphireVert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SaphireVert/gitlabot/issues"
  },
  "homepage": "https://github.com/SaphireVert/gitlabot#readme",
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": false,
    "singleQuote": true,
    "printWidth": 200
  }
}
